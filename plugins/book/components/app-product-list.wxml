<view class="goods-list dir-top-nowrap data-v-21f3cefb"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="row dir-left-nowrap main-between box-grow-1 data-v-21f3cefb"><block wx:for="{{item}}" wx:for-item="good" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['route_go',['$0'],[[['goodsList','',key],['','',index]]]]]]]}}" class="item box-grow-0 data-v-21f3cefb" bindtap="__e"><view class="image-name dir-top-nowrap main-left data-v-21f3cefb"><block wx:if="{{good.goods_stock==0&&appSetting.is_show_stock=='1'}}"><view class="out-dialog data-v-21f3cefb"><image src="{{appSetting.is_use_stock=='1'?appImg.plugins_out:appSetting.sell_out_pic}}" class="data-v-21f3cefb"></image></view></block><image class="cover_pic data-v-21f3cefb" lazy-load="{{true}}" mode="aspectFill" src="{{good.cover_pic}}"></image><view class="name t-omit data-v-21f3cefb">{{good.name}}</view></view><view class="content dir-top-nowrap main-right data-v-21f3cefb"><block wx:if="{{good.is_level==1}}"><view class="price-bottom data-v-21f3cefb"><app-member-price vue-id="{{'1-'+key+'-'+index}}" price="{{good.level_price}}" class="data-v-21f3cefb" bind:__l="__l"></app-member-price></view></block><block wx:if="{{good.discount>0}}"><view class="data-v-21f3cefb"><app-sup-vip vue-id="{{'2-'+key+'-'+index}}" is_vip_card_user="{{good.is_vip_card_user}}" discount="{{good.discount}}" class="data-v-21f3cefb" bind:__l="__l"></app-sup-vip></view></block><view class="{{['price-btn dir-left-nowrap main-between cross-center data-v-21f3cefb',theme]}}"><view class="{{['price data-v-21f3cefb',theme+'-m-text '+theme]}}">{{good.price_str}}</view><block wx:if="{{good.goods_stock!=0}}"><view class="{{['btn data-v-21f3cefb',theme+' '+theme+'-m-text '+theme+'-m-border']}}">预约</view></block></view></view></view></block></view></block></view>