<app-layout vue-id="1" class="data-v-18ca7e30" bind:__l="__l" vue-slots="{{['default']}}"><view class="head-card data-v-18ca7e30" style="{{'background-color:'+(''+list.form.card_bg)+';'}}"><image class="card-bottom data-v-18ca7e30" src="{{svipImg.card_bottom}}" alt></image></view><view class="list data-v-18ca7e30"><view class="title main-center cross-center data-v-18ca7e30"><image src="{{svipImg.left}}" alt class="data-v-18ca7e30"></image><view class="title-text data-v-18ca7e30">购买会员</view><image src="{{svipImg.right}}" alt class="data-v-18ca7e30"></image></view><view class="item dir-left-nowrap data-v-18ca7e30" style="border-top:0;"><view class="label data-v-18ca7e30">超级会员卡名称</view><view class="content data-v-18ca7e30">{{list.name}}</view></view><view class="item dir-left-nowrap data-v-18ca7e30"><view class="label data-v-18ca7e30">会员卡标题</view><view class="content data-v-18ca7e30">{{card.name}}</view></view><view class="item dir-left-nowrap data-v-18ca7e30"><view class="label data-v-18ca7e30">有效时间</view><view class="content data-v-18ca7e30">{{(end_time>0?end_time:today)+"-"+end_day}}</view></view><view class="item dir-left-nowrap data-v-18ca7e30"><view class="label data-v-18ca7e30">有效期</view><view class="content data-v-18ca7e30">{{card.expire_day+"天"}}</view></view><view class="item dir-left-nowrap data-v-18ca7e30"><view class="label data-v-18ca7e30">会员卡权益</view><view class="content cross-center main-between data-v-18ca7e30"><view style="min-width:70%;" class="data-v-18ca7e30"><block wx:if="{{card.main.is_free_delivery==1}}"><view class="data-v-18ca7e30">自营商品包邮</view></block><block wx:if="{{card.main.is_discount&&card.type_info.all}}"><view class="data-v-18ca7e30"><text class="data-v-18ca7e30">{{"自营商品"+card.main.discount+"折"}}</text><block wx:if="{{card.type_info.ignore_goods.length>0||card.type_info.ignore_cats.length>0}}"><text class="about data-v-18ca7e30">(特殊商品除外)</text></block></view></block><block wx:if="{{card.main.is_discount&&card.type_info.cats.length>0}}"><view class="data-v-18ca7e30">{{"指定分类"+card.main.discount+"折"}}</view></block><block wx:if="{{card.main.is_discount&&card.type_info.goods.length>0}}"><view class="data-v-18ca7e30">{{"指定商品"+card.main.discount+"折"}}</view></block></view><view data-event-opts="{{[['tap',[['toDetail',['$event']]]]]}}" class="to-detail main-center cross-center data-v-18ca7e30" bindtap="__e"><view class="data-v-18ca7e30">详情</view><image src="/static/image/icon/arrow-right.png" class="data-v-18ca7e30"></image></view></view></view><block wx:if="{{card.send_integral_num>0||card.send_balance>0||card.coupons.length>0||card.cards.length>0}}"><view class="item dir-left-nowrap data-v-18ca7e30"><view class="label data-v-18ca7e30">购买即赠</view><view class="content data-v-18ca7e30"><block wx:if="{{card.send_integral_num>0}}"><view class="data-v-18ca7e30">{{"积分"+card.send_integral_num}}</view></block><block wx:if="{{card.send_balance>0}}"><view class="data-v-18ca7e30">{{"余额"+card.send_balance}}</view></block><block wx:for="{{card.coupons}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><block wx:if="{{card.coupons.length>0}}"><view class="data-v-18ca7e30">{{item.name+"优惠券"+item.send_num+"张"}}</view></block></block><block wx:for="{{card.cards}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><block wx:if="{{card.cards.length>0}}"><view class="data-v-18ca7e30">{{item.name+"卡券"+item.send_num+"张"}}</view></block></block></view></view></block></view><view class="{{['data-v-18ca7e30','bottom-place',''+(iphone_x?'iphone_x':'')]}}"></view><view class="{{['data-v-18ca7e30','foot',''+(iphone_x?'iphone_x':'')]}}"><view data-event-opts="{{[['tap',[['subscribe',['$event']]]]]}}" class="foot-button data-v-18ca7e30" bindtap="__e">支付</view></view><block wx:if="{{success}}"><view class="dialog main-center cross-center data-v-18ca7e30"><view class="gift data-v-18ca7e30"><image class="gift-bg data-v-18ca7e30" src="./../image/gift.png"></image><view class="gift-list data-v-18ca7e30"><block wx:if="{{card.send_integral_num>0}}"><view data-event-opts="{{[['tap',[['toIntegral',['$event']]]]]}}" class="gift-item data-v-18ca7e30" bindtap="__e"><image class="gift-icon data-v-18ca7e30" src="{{list.form.integral_icon!='statics/img/app/vip_card/integral.png'?list.form.integral_icon:svipImg.integral}}"></image><view class="data-v-18ca7e30">{{"积分"+card.send_integral_num}}</view><image src="/static/image/icon/arrow-right.png" class="data-v-18ca7e30"></image></view></block><block wx:if="{{card.send_balance>0}}"><view data-event-opts="{{[['tap',[['toBalance',['$event']]]]]}}" class="gift-item data-v-18ca7e30" bindtap="__e"><image class="gift-icon data-v-18ca7e30" src="{{list.form.balance_icon!='statics/img/app/vip_card/balance.png'?list.form.balance_icon:svipImg.balance}}"></image><view class="data-v-18ca7e30">{{"余额"+card.send_balance}}</view><image src="/static/image/icon/arrow-right.png" class="data-v-18ca7e30"></image></view></block><block wx:if="{{card.coupons.length>0}}"><view data-event-opts="{{[['tap',[['toCoupon',['$event']]]]]}}" class="gift-item data-v-18ca7e30" bindtap="__e"><image class="gift-icon data-v-18ca7e30" src="{{list.form.coupon_icon!='statics/img/app/vip_card/coupon.png'?list.form.coupon_icon:svipImg.coupon}}"></image><view class="data-v-18ca7e30">{{"优惠券"+total_coupon+"张"}}</view><image src="/static/image/icon/arrow-right.png" class="data-v-18ca7e30"></image></view></block><block wx:if="{{card.cards.length>0}}"><view data-event-opts="{{[['tap',[['toCard',['$event']]]]]}}" class="gift-item data-v-18ca7e30" bindtap="__e"><image class="gift-icon data-v-18ca7e30" src="{{list.form.card_icon!='statics/img/app/vip_card/card.png'?list.form.card_icon:svipImg.card}}"></image><view class="data-v-18ca7e30">{{"卡券"+total_card+"张"}}</view><image src="/static/image/icon/arrow-right.png" class="data-v-18ca7e30"></image></view></block><view data-event-opts="{{[['tap',[['toBack',['$event']]]]]}}" class="dialog-btn data-v-18ca7e30" bindtap="__e">我知道了</view></view></view></view></block></app-layout>